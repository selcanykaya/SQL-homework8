3-
UPDATE employee
SET name = 'Onur'
    email = 'onur@mail.com'
WHERE id = 7;


UPDATE employee
SET name = 'Selcan'
    birtday = '1998-02-03'
WHERE name LIKE 'C%';


UPDATE employee
SET name = 'Dhorlas'
WHERE email = 'baucock15@umn.edu';


UPDATE employee
SET email = 'dhorlasck15@umn.edu'
WHERE id = 13;


UPDATE employee
SET name = 'Lorel'
    birtday = '1986-02-13'
WHERE name = 'Gertrude'
RETURNING *;


4-
DELETE FROM employee
WHERE name = 'Onur';

DELETE FROM employee
WHERE id = 15;

DELETE FROM employee
WHERE email = 'baucock15@umn.edu';

DELETE FROM employee
WHERE birthday = '1977-10-24';

DELETE FROM employee
WHERE id > 30
RETURNING *;